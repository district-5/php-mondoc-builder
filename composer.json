{
  "name": "district5/mondoc-builder",
  "type": "library",
  "description": "A simple to use object orientated library to generate queries to use in MongoDB",
  "keywords": [
    "district5",
    "mongo",
    "mongodb",
    "query",
    "builder",
    "database",
    "db"
  ],
  "homepage": "https://github.com/district-5/php-mondoc-builder",
  "license": "mit",
  "authors": [
    {
      "name": "District5",
      "email": "hello@district5.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "District5\\MondocBuilder\\": "src/MondocBuilder/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "District5Tests\\MondocBuilderTests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.4",
    "mongodb/mongodb": "*",
    "ext-mongodb": "*",
    "district5/date": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}